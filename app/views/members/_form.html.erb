<%= form_for @member do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :matric %>
  <%= f.text_field :matric %><br>
  <%= f.label :contact %>
  <%= f.text_field :contact %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :cell %>
  <%= f.text_field :cell %><br>
  <%= f.label :MC %>
  <%= f.text_field :MC %><br>
  <%= f.submit "Submit" %>
<% end %>
  