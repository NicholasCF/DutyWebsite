<p id="notice"><%= notice %></p>

<h1>List of Members</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Matriculation no.</th>
      <th>Contact no.</th>
      <th>Email</th>
      <th>Cell</th>
      <th>Position</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.matric %></td>
        <td><%= member.contact %></td>
        <td><%= member.email %></td>
        <td><%= member.cell %></td>
        <% if member.MC %>
          <td><%= MC %></td>
        <% else %>
          <td><%= Member %></td>
        <% end %>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Delete', member, method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'New Member', new_member_path %>
<br>